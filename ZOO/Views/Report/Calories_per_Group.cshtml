@model IEnumerable<ZOO.Models.Calories_per_Group1_Result>

@{
    ViewBag.Title = "Calories_per_Group";
    Layout = "~/Views/Shared/_LayoutReports.cshtml";

}
@{
    var DateFrom = "";
    var DateTo = "";

    if (IsPost)
    {
        DateFrom = Request.Form["date"];
        DateTo = Request.Form["date"];
    }
}
<br />
<br />
<br />
<br />
<br />
<div class="form-group">
    <div class="col-md-10">
        <p>
            <label for="DateFrom">DateFrom:</label>
            <input type="date" name="DateFrom" value="@Request.Form["date"]" />
        </p>
    </div>
</div>

<div class="form-group">
    <div class="col-md-10">
        <p>
            <label for="DateTo">DateTo:</label>
            <input type="date" name="DateTo" value="@Request.Form["date"]" />
        </p>
    </div>
</div>


<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <li>@Html.ActionLink("Calories Report", "Calories_per_Group", "Report", new { datefrom = DateFrom, dateTo = DateTo }, null)</li>
    </div>
</div
<br />
<br />
<br />
<br />
<br />
<br />
<h2>Calories_per_Group</h2>


<table class="table" border="1" style="background-color:white;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ElementAt(0).Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.ElementAt(0).Eaten)
        </th>



    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eaten)
            </td>
 


        </tr>
    }
 </table>





