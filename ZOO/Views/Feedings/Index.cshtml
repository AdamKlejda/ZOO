@model IEnumerable<ZOO.Models.Feedings>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TimeForFeeding)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FeedingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnimalGroups.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employees.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FoodProducts.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TimeForFeeding)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FeedingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnimalGroups.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employees.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoodProducts.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FeedingId }) |
            @Html.ActionLink("Details", "Details", new { id=item.FeedingId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FeedingId })
        </td>
    </tr>
}

</table>
